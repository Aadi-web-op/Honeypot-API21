{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "setup": {
          "nixPkgs": ["tesseract", "python311"]
        },
        "install": {
          "cmds": ["python -m spacy download en_core_web_lg"] 
        }
      }
    }
  },
  "deploy": {
    "startCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT"
  }
}